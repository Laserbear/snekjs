{
  "manifest_version": 2,

  "name": "snek.js",
  "description": "Slither.io bot",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "content_scripts": [
  {
    "matches": [
      "http://slither.io/"
    ],
    "js": ["content.js","all.js", "rl.js"]
  }
],
  "permissions": [
   "activeTab"
   ]
}
